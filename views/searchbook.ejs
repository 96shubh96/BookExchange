<script>
function notifyMe() {
  if (!("Notification" in window)) {
    alert("This browser does not support desktop notification");
  }
  else if (Notification.permission === "granted") {
        var options = {
                body: "Your request has been successfully sent....",
                icon: "https://upload.wikimedia.org/wikipedia/commons/thumb/2/25/Book_icon_(closed)_-_Red_and_gold.svg/200px-Book_icon_(closed)_-_Red_and_gold.svg.png",
                dir : "auto"
             };
          var notification = new Notification("Hey!!!",options);
  }
  else if (Notification.permission !== 'denied') {
    Notification.requestPermission(function (permission) {
      if (!('permission' in Notification)) {
        Notification.permission = permission;
      }
    
      if (permission === "granted") {
        var options = {
              body: "This is the body of the notification",
              icon: "icon.jpg",
              dir : "auto"
          };
        var notification = new Notification("Hi there",options);
      }
    });
  }
}
</script>
