	<h2 class="center-align">Edit Profile</h2><br><br>
	 <div class="row">
    <form class="col s4 offset-s4 #f5f5f5 grey lighten-4" method="POST" action="editdata">
      <div class="row">
        <div class="input-field col s12">
          <input id="roll_no" type="text" class="validate" name="rollno" value="<%=roll%>"disabled selected>
          <label for="roll_no">Roll No</label>
        </div>
      </div>
	  <div class="row">
        <div class="input-field col s6 ">
          <input id="first_name" type="text" class="validate" name="firstname" value="<%=name%>"disabled selected>
          <label for="first_name">First Name</label>
        </div>
        <div class="input-field col s6">
          <input id="last_name" type="text" class="validate" name="lastname" value="<%=name1%>"disabled selected>
          <label for="last_name">Last Name</label>
        </div>
      </div>
      
      <div class="row">
        <div class="input-field col s12">
          <input id="branch" type="text" class="validate" name="branch" value="<%=branch%>">
          <label for="branch">Branch</label>
        </div>
      </div>
	 <div class="input-field col s12">
	  
    <select class="browser-default" name="sem">
      <option value="" disabled selected><%=sem%></option>
      <option value="1">1st Sem</option>
      <option value="2">2nd Sem</option>
      <option value="3">3rd Sem</option>
      <option value="4">4th Sem</option>
	  <option value="5">5th Sem</option>
	  <option value="6">6th Sem</option>
	  <option value="7">7th Sem</option>
	  <option value="8">8th Sem</option>
    </select>
            </div>
<br><br><br>
			<div class="input-field col s12">
          <input id="contact_no" type="tel" class="validate" name="contact" value="<%=contact%>">
          <label for="contact_no">Contact No</label>
        </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="email" type="email" class="validate" name="email" value="<%=email%>">
          <label for="email">Email</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="password" type="password" class="validate" name="password">
          <label for="password">Password</label>
        </div>
      </div>
	        <div class="row">
        <div class="input-field col s12">
          <input id="confirmpassword" type="password" class="validate" name="confirmpassword">
          <label for="confirmpassword">Confirm Password</label>
        </div>
      </div>
 <button class="btn waves-effect waves-light col s12" type="submit" value="submit" href="profilepage">Update
   
  </button>
 
  </form>
